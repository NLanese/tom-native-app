{
  "expo": {
    "name": "The Tom App",
    "slug": "the-tom-app",
    "version": "1.0.0",
    "privacy": "public",
    "description": "In the development and testing phase",
    "orientation": "portrait",
    "icon": "./assets/TomSplash.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }

  }
}
 